{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "types": ["vite/client", "node"],
    "skipLibCheck": true,
